provider "opc" {
    endpoint = "{{ endpoint }}"
    identity_domain = "{{ identity_domain }}"
    insecure = "{{ insecure }}"
    lbaas_endpoint = "{{ lbaas_endpoint }}"
    max_retries = "{{ max_retries }}"
    password = "{{ password }}"
    storage_endpoint = "{{ storage_endpoint }}"
    storage_service_id = "{{ storage_service_id }}"
    user = "{{ user }}"
}