provider "nomad" {
    address = "{{ address }}"
    ca_file = "{{ ca_file }}"
    ca_pem = "{{ ca_pem }}"
    cert_file = "{{ cert_file }}"
    cert_pem = "{{ cert_pem }}"
    consul_token = "{{ consul_token }}"
    http_auth = "{{ http_auth }}"
    ignore_env_vars = "{{ ignore_env_vars }}"
    key_file = "{{ key_file }}"
    key_pem = "{{ key_pem }}"
    region = "{{ region }}"
    secret_id = "{{ secret_id }}"
    skip_verify = "{{ skip_verify }}"
    vault_token = "{{ vault_token }}"
}