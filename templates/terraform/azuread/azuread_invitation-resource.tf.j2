resource "azuread_invitation" "example" {
    id = "{{ id }}"
    redeem_url = "{{ redeem_url }}"
    redirect_url = "{{ redirect_url }}"
    user_display_name = "{{ user_display_name }}"
    user_email_address = "{{ user_email_address }}"
    user_id = "{{ user_id }}"
    user_type = "{{ user_type }}"
}