resource "google_compute_target_instance" "example" {
    creation_timestamp = "{{ creation_timestamp }}"
    description = "{{ description }}"
    id = "{{ id }}"
    instance = "{{ instance }}"
    name = "{{ name }}"
    nat_policy = "{{ nat_policy }}"
    network = "{{ network }}"
    project = "{{ project }}"
    security_policy = "{{ security_policy }}"
    self_link = "{{ self_link }}"
    zone = "{{ zone }}"
}