    name = "{{ name }}"
    resource_group_name = "{{ resource_group_name }}"
    tags = "{{ tags }}"
    vpn_authentication_types = "{{ vpn_authentication_types }}"
    vpn_protocols = "{{ vpn_protocols }}"
}