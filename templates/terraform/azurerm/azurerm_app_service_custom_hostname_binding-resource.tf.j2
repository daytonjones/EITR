resource "azurerm_app_service_custom_hostname_binding" "example" {
    app_service_name = "{{ app_service_name }}"
    hostname = "{{ hostname }}"
    id = "{{ id }}"
    resource_group_name = "{{ resource_group_name }}"
    ssl_state = "{{ ssl_state }}"
    thumbprint = "{{ thumbprint }}"
    virtual_ip = "{{ virtual_ip }}"
}