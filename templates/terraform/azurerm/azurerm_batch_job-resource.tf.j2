resource "azurerm_batch_job" "example" {
    batch_pool_id = "{{ batch_pool_id }}"
    common_environment_properties = "{{ common_environment_properties }}"
    display_name = "{{ display_name }}"
    id = "{{ id }}"
    name = "{{ name }}"
    priority = "{{ priority }}"
    task_retry_maximum = "{{ task_retry_maximum }}"
}