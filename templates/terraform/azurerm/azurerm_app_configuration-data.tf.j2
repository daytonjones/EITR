data "azurerm_app_configuration" "example" {
    encryption = "{{ encryption }}"
    endpoint = "{{ endpoint }}"
    id = "{{ id }}"
    identity = "{{ identity }}"
    local_auth_enabled = "{{ local_auth_enabled }}"
    location = "{{ location }}"
    name = "{{ name }}"
    primary_read_key = "{{ primary_read_key }}"
    primary_write_key = "{{ primary_write_key }}"
    public_network_access = "{{ public_network_access }}"
    public_network_access_enabled = "{{ public_network_access_enabled }}"
    purge_protection_enabled = "{{ purge_protection_enabled }}"
    replica = "{{ replica }}"
    resource_group_name = "{{ resource_group_name }}"
    secondary_read_key = "{{ secondary_read_key }}"
    secondary_write_key = "{{ secondary_write_key }}"
    sku = "{{ sku }}"
    soft_delete_retention_days = "{{ soft_delete_retention_days }}"
    tags = "{{ tags }}"
}