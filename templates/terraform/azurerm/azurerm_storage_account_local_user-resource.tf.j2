resource "azurerm_storage_account_local_user" "example" {
    home_directory = "{{ home_directory }}"
    id = "{{ id }}"
    name = "{{ name }}"
    password = "{{ password }}"
    sid = "{{ sid }}"
    ssh_key_enabled = "{{ ssh_key_enabled }}"
    ssh_password_enabled = "{{ ssh_password_enabled }}"
    storage_account_id = "{{ storage_account_id }}"
}