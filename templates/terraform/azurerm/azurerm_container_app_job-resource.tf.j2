resource "azurerm_container_app_job" "example" {
    container_app_environment_id = "{{ container_app_environment_id }}"
    event_stream_endpoint = "{{ event_stream_endpoint }}"
    id = "{{ id }}"
    location = "{{ location }}"
    name = "{{ name }}"
    outbound_ip_addresses = "{{ outbound_ip_addresses }}"
    replica_retry_limit = "{{ replica_retry_limit }}"
    replica_timeout_in_seconds = "{{ replica_timeout_in_seconds }}"
    resource_group_name = "{{ resource_group_name }}"
    tags = "{{ tags }}"
    workload_profile_name = "{{ workload_profile_name }}"
}