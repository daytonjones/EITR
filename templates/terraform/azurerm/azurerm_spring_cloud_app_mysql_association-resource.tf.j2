resource "azurerm_spring_cloud_app_mysql_association" "example" {
    database_name = "{{ database_name }}"
    id = "{{ id }}"
    mysql_server_id = "{{ mysql_server_id }}"
    name = "{{ name }}"
    password = "{{ password }}"
    spring_cloud_app_id = "{{ spring_cloud_app_id }}"
    username = "{{ username }}"
}