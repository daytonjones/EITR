resource "azurerm_static_site" "example" {
    api_key = "{{ api_key }}"
    app_settings = "{{ app_settings }}"
    default_host_name = "{{ default_host_name }}"
    id = "{{ id }}"
    location = "{{ location }}"
    name = "{{ name }}"
    resource_group_name = "{{ resource_group_name }}"
    sku_size = "{{ sku_size }}"
    sku_tier = "{{ sku_tier }}"
    tags = "{{ tags }}"
}