resource "azurerm_route_table" "example" {
    bgp_route_propagation_enabled = "{{ bgp_route_propagation_enabled }}"
    id = "{{ id }}"
    location = "{{ location }}"
    name = "{{ name }}"
    resource_group_name = "{{ resource_group_name }}"
    route = "{{ route }}"
    subnets = "{{ subnets }}"
    tags = "{{ tags }}"
}