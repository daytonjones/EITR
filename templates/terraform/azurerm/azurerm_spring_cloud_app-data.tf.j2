data "azurerm_spring_cloud_app" "example" {
    fqdn = "{{ fqdn }}"
    https_only = "{{ https_only }}"
    id = "{{ id }}"
    identity = "{{ identity }}"
    is_public = "{{ is_public }}"
    name = "{{ name }}"
    persistent_disk = "{{ persistent_disk }}"
    resource_group_name = "{{ resource_group_name }}"
    service_name = "{{ service_name }}"
    tls_enabled = "{{ tls_enabled }}"
    url = "{{ url }}"
}