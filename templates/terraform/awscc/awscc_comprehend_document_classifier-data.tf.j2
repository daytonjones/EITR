data "awscc_comprehend_document_classifier" "example" {
    arn = "{{ arn }}"
    data_access_role_arn = "{{ data_access_role_arn }}"
    document_classifier_name = "{{ document_classifier_name }}"
    id = "{{ id }}"
    input_data_config = "{
    augmented_manifests = {
    attribute_names = {{ attribute_names }}
    s3_uri = {{ s3_uri }}
    split = {{ split }}
}
    data_format = {{ data_format }}
    document_reader_config = {
    document_read_action = {{ document_read_action }}
    document_read_mode = {{ document_read_mode }}
    feature_types = {{ feature_types }}
}
    document_type = {{ document_type }}
    documents = {
    s3_uri = {{ s3_uri }}
    test_s3_uri = {{ test_s3_uri }}
}
    label_delimiter = {{ label_delimiter }}
    s3_uri = {{ s3_uri }}
    test_s3_uri = {{ test_s3_uri }}
}"
    language_code = "{{ language_code }}"
    mode = "{{ mode }}"
    model_kms_key_id = "{{ model_kms_key_id }}"
    model_policy = "{{ model_policy }}"
    output_data_config = "{
    kms_key_id = {{ kms_key_id }}
    s3_uri = {{ s3_uri }}
}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
    version_name = "{{ version_name }}"
    volume_kms_key_id = "{{ volume_kms_key_id }}"
    vpc_config = "{
    security_group_ids = {{ security_group_ids }}
    subnets = {{ subnets }}
}"
}