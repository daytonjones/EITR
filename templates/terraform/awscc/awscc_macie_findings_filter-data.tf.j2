data "awscc_macie_findings_filter" "example" {
    action = "{{ action }}"
    arn = "{{ arn }}"
    description = "{{ description }}"
    finding_criteria = "{
    criterion = {
    eq = {{ eq }}
    gt = {{ gt }}
    gte = {{ gte }}
    lt = {{ lt }}
    lte = {{ lte }}
    neq = {{ neq }}
}
}"
    findings_filter_id = "{{ findings_filter_id }}"
    id = "{{ id }}"
    name = "{{ name }}"
    position = "{{ position }}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
}