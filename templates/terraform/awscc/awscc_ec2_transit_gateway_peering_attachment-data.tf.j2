data "awscc_ec2_transit_gateway_peering_attachment" "example" {
    creation_time = "{{ creation_time }}"
    id = "{{ id }}"
    peer_account_id = "{{ peer_account_id }}"
    peer_region = "{{ peer_region }}"
    peer_transit_gateway_id = "{{ peer_transit_gateway_id }}"
    state = "{{ state }}"
    status = "{
    code = {{ code }}
