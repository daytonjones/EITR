}
    scopes = {
    destination_ports = {
    from_port = {{ from_port }}
    to_port = {{ to_port }}
}
    destinations = {
    address_definition = {{ address_definition }}
}
    protocols = {{ protocols }}
    source_ports = {
    from_port = {{ from_port }}
    to_port = {{ to_port }}
}
    sources = {
    address_definition = {{ address_definition }}
}
}
    server_certificates = {
    resource_arn = {{ resource_arn }}
}
}
}"
