resource "awscc_sagemaker_inference_experiment" "example" {
    arn = "{{ arn }}"
    creation_time = "{{ creation_time }}"
    data_storage_config = "{
    content_type = {
    csv_content_types = {{ csv_content_types }}
    json_content_types = {{ json_content_types }}
}
    destination = {{ destination }}
    kms_key = {{ kms_key }}
}"
    description = "{{ description }}"
    desired_state = "{{ desired_state }}"
    endpoint_metadata = "{
    endpoint_config_name = {{ endpoint_config_name }}
    endpoint_name = {{ endpoint_name }}
    endpoint_status = {{ endpoint_status }}
}"
    endpoint_name = "{{ endpoint_name }}"
    id = "{{ id }}"
    kms_key = "{{ kms_key }}"
    last_modified_time = "{{ last_modified_time }}"
    model_variants = "{
    infrastructure_config = {
    infrastructure_type = {{ infrastructure_type }}
    real_time_inference_config = {
    instance_count = {{ instance_count }}
    instance_type = {{ instance_type }}
}
}
    model_name = {{ model_name }}
    variant_name = {{ variant_name }}
}"
    name = "{{ name }}"
    role_arn = "{{ role_arn }}"
    schedule = "{
    end_time = {{ end_time }}
    start_time = {{ start_time }}
}"
    shadow_mode_config = "{
    shadow_model_variants = {
    sampling_percentage = {{ sampling_percentage }}
    shadow_model_variant_name = {{ shadow_model_variant_name }}
}
    source_model_variant_name = {{ source_model_variant_name }}
}"
    status = "{{ status }}"
    status_reason = "{{ status_reason }}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
    type = "{{ type }}"
}