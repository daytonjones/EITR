data "awscc_appconfig_environment" "example" {
    application_id = "{{ application_id }}"
    description = "{{ description }}"
    environment_id = "{{ environment_id }}"
    id = "{{ id }}"
    monitors = "{
    alarm_arn = {{ alarm_arn }}
    alarm_role_arn = {{ alarm_role_arn }}
}"
    name = "{{ name }}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
}