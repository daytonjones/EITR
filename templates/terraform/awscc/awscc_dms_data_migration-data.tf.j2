data "awscc_dms_data_migration" "example" {
    data_migration_arn = "{{ data_migration_arn }}"
    data_migration_create_time = "{{ data_migration_create_time }}"
    data_migration_identifier = "{{ data_migration_identifier }}"
    data_migration_name = "{{ data_migration_name }}"
    data_migration_settings = "{
    cloudwatch_logs_enabled = {{ cloudwatch_logs_enabled }}
    number_of_jobs = {{ number_of_jobs }}
    selection_rules = {{ selection_rules }}
}"
    data_migration_type = "{{ data_migration_type }}"
    id = "{{ id }}"
    migration_project_identifier = "{{ migration_project_identifier }}"
    service_access_role_arn = "{{ service_access_role_arn }}"
    source_data_settings = "{
    cdc_start_position = {{ cdc_start_position }}
    cdc_start_time = {{ cdc_start_time }}
    cdc_stop_time = {{ cdc_stop_time }}
    slot_name = {{ slot_name }}
}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
}