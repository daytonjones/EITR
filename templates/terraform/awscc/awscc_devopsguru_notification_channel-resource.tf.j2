resource "awscc_devopsguru_notification_channel" "example" {
    config = "{
    filters = {
    message_types = {{ message_types }}
    severities = {{ severities }}
}
    sns = {
    topic_arn = {{ topic_arn }}
}
}"
    id = "{{ id }}"
    notification_channel_id = "{{ notification_channel_id }}"
}