resource "awscc_logs_log_group" "example" {
    arn = "{{ arn }}"
    data_protection_policy = "{{ data_protection_policy }}"
    id = "{{ id }}"
    kms_key_id = "{{ kms_key_id }}"
    log_group_class = "{{ log_group_class }}"
    log_group_name = "{{ log_group_name }}"
    retention_in_days = "{{ retention_in_days }}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
}