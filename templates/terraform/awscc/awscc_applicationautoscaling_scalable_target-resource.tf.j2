resource "awscc_applicationautoscaling_scalable_target" "example" {
    id = "{{ id }}"
    max_capacity = "{{ max_capacity }}"
    min_capacity = "{{ min_capacity }}"
    resource_id = "{{ resource_id }}"
    role_arn = "{{ role_arn }}"
    scalable_dimension = "{{ scalable_dimension }}"
    scalable_target_id = "{{ scalable_target_id }}"
    scheduled_actions = "{
    end_time = {{ end_time }}
    scalable_target_action = {
    max_capacity = {{ max_capacity }}
    min_capacity = {{ min_capacity }}
}
    schedule = {{ schedule }}
    scheduled_action_name = {{ scheduled_action_name }}
    start_time = {{ start_time }}
    timezone = {{ timezone }}
}"
    service_namespace = "{{ service_namespace }}"
    suspended_state = "{
    dynamic_scaling_in_suspended = {{ dynamic_scaling_in_suspended }}
    dynamic_scaling_out_suspended = {{ dynamic_scaling_out_suspended }}
    scheduled_scaling_suspended = {{ scheduled_scaling_suspended }}
}"
}