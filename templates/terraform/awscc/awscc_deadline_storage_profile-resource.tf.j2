resource "awscc_deadline_storage_profile" "example" {
    display_name = "{{ display_name }}"
    farm_id = "{{ farm_id }}"
    file_system_locations = "{
    name = {{ name }}
    path = {{ path }}
    type = {{ type }}
}"
    id = "{{ id }}"
    os_family = "{{ os_family }}"
    storage_profile_id = "{{ storage_profile_id }}"
}