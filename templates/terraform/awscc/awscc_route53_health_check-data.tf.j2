data "awscc_route53_health_check" "example" {
    health_check_config = "{
    alarm_identifier = {
    name = {{ name }}
    region = {{ region }}
}
    child_health_checks = {{ child_health_checks }}
    enable_sni = {{ enable_sni }}
    failure_threshold = {{ failure_threshold }}
    fully_qualified_domain_name = {{ fully_qualified_domain_name }}
    health_threshold = {{ health_threshold }}
    insufficient_data_health_status = {{ insufficient_data_health_status }}
    inverted = {{ inverted }}
    ip_address = {{ ip_address }}
    measure_latency = {{ measure_latency }}
    port = {{ port }}
    regions = {{ regions }}
    request_interval = {{ request_interval }}
    resource_path = {{ resource_path }}
    routing_control_arn = {{ routing_control_arn }}
    search_string = {{ search_string }}
    type = {{ type }}
}"
    health_check_id = "{{ health_check_id }}"
    health_check_tags = "{
    key = {{ key }}
    value = {{ value }}
}"
    id = "{{ id }}"
}