resource "awscc_ssmincidents_replication_set" "example" {
    arn = "{{ arn }}"
    deletion_protected = "{{ deletion_protected }}"
    id = "{{ id }}"
    regions = "{
    region_configuration = {
    sse_kms_key_id = {{ sse_kms_key_id }}
}
    region_name = {{ region_name }}
}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
}