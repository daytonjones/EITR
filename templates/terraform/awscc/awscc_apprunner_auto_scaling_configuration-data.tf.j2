data "awscc_apprunner_auto_scaling_configuration" "example" {
    auto_scaling_configuration_arn = "{{ auto_scaling_configuration_arn }}"
    auto_scaling_configuration_name = "{{ auto_scaling_configuration_name }}"
    auto_scaling_configuration_revision = "{{ auto_scaling_configuration_revision }}"
    id = "{{ id }}"
    latest = "{{ latest }}"
    max_concurrency = "{{ max_concurrency }}"
    max_size = "{{ max_size }}"
    min_size = "{{ min_size }}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
}