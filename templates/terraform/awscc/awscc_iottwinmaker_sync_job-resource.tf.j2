resource "awscc_iottwinmaker_sync_job" "example" {
    arn = "{{ arn }}"
    creation_date_time = "{{ creation_date_time }}"
    id = "{{ id }}"
    state = "{{ state }}"
    sync_role = "{{ sync_role }}"
    sync_source = "{{ sync_source }}"
    tags = "{{ tags }}"
    update_date_time = "{{ update_date_time }}"
    workspace_id = "{{ workspace_id }}"
}