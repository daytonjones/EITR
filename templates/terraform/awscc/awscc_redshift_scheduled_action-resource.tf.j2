resource "awscc_redshift_scheduled_action" "example" {
    enable = "{{ enable }}"
    end_time = "{{ end_time }}"
    iam_role = "{{ iam_role }}"
    id = "{{ id }}"
    next_invocations = "{{ next_invocations }}"
    schedule = "{{ schedule }}"
    scheduled_action_description = "{{ scheduled_action_description }}"
    scheduled_action_name = "{{ scheduled_action_name }}"
    start_time = "{{ start_time }}"
    state = "{{ state }}"
    target_action = "{
    pause_cluster = {
    cluster_identifier = {{ cluster_identifier }}
}
    resize_cluster = {
    classic = {{ classic }}
    cluster_identifier = {{ cluster_identifier }}
    cluster_type = {{ cluster_type }}
    node_type = {{ node_type }}
    number_of_nodes = {{ number_of_nodes }}
}
    resume_cluster = {
    cluster_identifier = {{ cluster_identifier }}
}
}"
}