data "awscc_eks_access_entry" "example" {
    access_entry_arn = "{{ access_entry_arn }}"
    access_policies = "{
    access_scope = {
    namespaces = {{ namespaces }}
    type = {{ type }}
}
    policy_arn = {{ policy_arn }}
}"
    cluster_name = "{{ cluster_name }}"
    id = "{{ id }}"
    kubernetes_groups = "{{ kubernetes_groups }}"
    principal_arn = "{{ principal_arn }}"
    tags = "{
    key = {{ key }}
    value = {{ value }}
}"
    type = "{{ type }}"
    username = "{{ username }}"
}