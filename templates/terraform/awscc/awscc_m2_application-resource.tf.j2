resource "awscc_m2_application" "example" {
    application_arn = "{{ application_arn }}"
    application_id = "{{ application_id }}"
    definition = "{
    content = {{ content }}
    s3_location = {{ s3_location }}
}"
    description = "{{ description }}"
    engine_type = "{{ engine_type }}"
    id = "{{ id }}"
    kms_key_id = "{{ kms_key_id }}"
    name = "{{ name }}"
    role_arn = "{{ role_arn }}"
    tags = "{{ tags }}"
}