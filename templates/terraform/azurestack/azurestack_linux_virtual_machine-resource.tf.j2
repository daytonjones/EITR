resource "azurestack_linux_virtual_machine" "example" {
    admin_password = "{{ admin_password }}"
    admin_username = "{{ admin_username }}"
    allow_extension_operations = "{{ allow_extension_operations }}"
    availability_set_id = "{{ availability_set_id }}"
