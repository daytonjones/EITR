resource "aws_glue_trigger" "example" {
    arn = "{{ arn }}"
    description = "{{ description }}"
    enabled = "{{ enabled }}"
    id = "{{ id }}"
    name = "{{ name }}"
    schedule = "{{ schedule }}"
    start_on_creation = "{{ start_on_creation }}"
    state = "{{ state }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
    type = "{{ type }}"
    workflow_name = "{{ workflow_name }}"
}