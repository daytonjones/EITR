resource "aws_ecr_repository" "example" {
    arn = "{{ arn }}"
    force_delete = "{{ force_delete }}"
    id = "{{ id }}"
    image_tag_mutability = "{{ image_tag_mutability }}"
    name = "{{ name }}"
    registry_id = "{{ registry_id }}"
    repository_url = "{{ repository_url }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}