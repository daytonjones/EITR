resource "aws_pipes_pipe" "example" {
    arn = "{{ arn }}"
    description = "{{ description }}"
    desired_state = "{{ desired_state }}"
    enrichment = "{{ enrichment }}"
    id = "{{ id }}"
    name = "{{ name }}"
    name_prefix = "{{ name_prefix }}"
    role_arn = "{{ role_arn }}"
    source = "{{ source }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
    target = "{{ target }}"
}