resource "aws_redshift_event_subscription" "example" {
    arn = "{{ arn }}"
    customer_aws_id = "{{ customer_aws_id }}"
    enabled = "{{ enabled }}"
    event_categories = "{{ event_categories }}"
    id = "{{ id }}"
    name = "{{ name }}"
    severity = "{{ severity }}"
    sns_topic_arn = "{{ sns_topic_arn }}"
    source_ids = "{{ source_ids }}"
    source_type = "{{ source_type }}"
    status = "{{ status }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}