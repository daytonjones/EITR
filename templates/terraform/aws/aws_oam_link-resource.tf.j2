resource "aws_oam_link" "example" {
    arn = "{{ arn }}"
    id = "{{ id }}"
    label = "{{ label }}"
    label_template = "{{ label_template }}"
    link_id = "{{ link_id }}"
    resource_types = "{{ resource_types }}"
    sink_arn = "{{ sink_arn }}"
    sink_identifier = "{{ sink_identifier }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}