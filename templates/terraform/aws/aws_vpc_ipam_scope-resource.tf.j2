resource "aws_vpc_ipam_scope" "example" {
    arn = "{{ arn }}"
    description = "{{ description }}"
    id = "{{ id }}"
    ipam_arn = "{{ ipam_arn }}"
    ipam_id = "{{ ipam_id }}"
    ipam_scope_type = "{{ ipam_scope_type }}"
    is_default = "{{ is_default }}"
    pool_count = "{{ pool_count }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}