data "aws_route53_zone" "example" {
    arn = "{{ arn }}"
    caller_reference = "{{ caller_reference }}"
    comment = "{{ comment }}"
    id = "{{ id }}"
    linked_service_description = "{{ linked_service_description }}"
    linked_service_principal = "{{ linked_service_principal }}"
    name = "{{ name }}"
