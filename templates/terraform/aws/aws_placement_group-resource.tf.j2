resource "aws_placement_group" "example" {
    arn = "{{ arn }}"
    id = "{{ id }}"
    name = "{{ name }}"
    partition_count = "{{ partition_count }}"
    placement_group_id = "{{ placement_group_id }}"
    spread_level = "{{ spread_level }}"
    strategy = "{{ strategy }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}