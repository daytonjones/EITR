resource "aws_s3control_bucket" "example" {
    arn = "{{ arn }}"
    bucket = "{{ bucket }}"
    creation_date = "{{ creation_date }}"
    id = "{{ id }}"
    outpost_id = "{{ outpost_id }}"
    public_access_block_enabled = "{{ public_access_block_enabled }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}