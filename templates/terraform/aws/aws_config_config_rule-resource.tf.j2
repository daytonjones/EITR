resource "aws_config_config_rule" "example" {
    arn = "{{ arn }}"
    description = "{{ description }}"
    id = "{{ id }}"
    input_parameters = "{{ input_parameters }}"
    maximum_execution_frequency = "{{ maximum_execution_frequency }}"
    name = "{{ name }}"
    rule_id = "{{ rule_id }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}