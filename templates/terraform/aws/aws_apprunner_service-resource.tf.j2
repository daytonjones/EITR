resource "aws_apprunner_service" "example" {
    arn = "{{ arn }}"
    auto_scaling_configuration_arn = "{{ auto_scaling_configuration_arn }}"
    id = "{{ id }}"
    service_id = "{{ service_id }}"
    service_name = "{{ service_name }}"
    service_url = "{{ service_url }}"
    status = "{{ status }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}