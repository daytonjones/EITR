resource "aws_appautoscaling_policy" "example" {
    alarm_arns = "{{ alarm_arns }}"
    arn = "{{ arn }}"
    id = "{{ id }}"
    name = "{{ name }}"
    policy_type = "{{ policy_type }}"
    resource_id = "{{ resource_id }}"
    scalable_dimension = "{{ scalable_dimension }}"
    service_namespace = "{{ service_namespace }}"
}