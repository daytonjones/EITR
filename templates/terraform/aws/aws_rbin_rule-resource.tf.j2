resource "aws_rbin_rule" "example" {
    arn = "{{ arn }}"
    description = "{{ description }}"
    id = "{{ id }}"
    lock_end_time = "{{ lock_end_time }}"
    lock_state = "{{ lock_state }}"
    resource_type = "{{ resource_type }}"
    status = "{{ status }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
}