    instance_platform = "{{ instance_platform }}"
    instance_type = "{{ instance_type }}"
    outpost_arn = "{{ outpost_arn }}"
    placement_group_arn = "{{ placement_group_arn }}"
    reservation_type = "{{ reservation_type }}"
    start_date = "{{ start_date }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
    tenancy = "{{ tenancy }}"
