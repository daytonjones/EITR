resource "aws_internetmonitor_monitor" "example" {
    arn = "{{ arn }}"
    id = "{{ id }}"
    max_city_networks_to_monitor = "{{ max_city_networks_to_monitor }}"
    monitor_name = "{{ monitor_name }}"
    resources = "{{ resources }}"
    status = "{{ status }}"
    tags = "{{ tags }}"
    tags_all = "{{ tags_all }}"
    traffic_percentage_to_monitor = "{{ traffic_percentage_to_monitor }}"
}