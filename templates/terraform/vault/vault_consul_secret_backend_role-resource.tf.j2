resource "vault_consul_secret_backend_role" "example" {
    backend = "{{ backend }}"
    consul_namespace = "{{ consul_namespace }}"
    consul_policies = "{{ consul_policies }}"
    consul_roles = "{{ consul_roles }}"
    id = "{{ id }}"
    local = "{{ local }}"
    max_ttl = "{{ max_ttl }}"
    name = "{{ name }}"
    namespace = "{{ namespace }}"
    node_identities = "{{ node_identities }}"
    partition = "{{ partition }}"
    policies = "{{ policies }}"
    service_identities = "{{ service_identities }}"
    ttl = "{{ ttl }}"
}