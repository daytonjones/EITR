resource "vault_azure_secret_backend_role" "example" {
    application_object_id = "{{ application_object_id }}"
    backend = "{{ backend }}"
    description = "{{ description }}"
    id = "{{ id }}"
    max_ttl = "{{ max_ttl }}"
    namespace = "{{ namespace }}"
    permanently_delete = "{{ permanently_delete }}"
    role = "{{ role }}"
    sign_in_audience = "{{ sign_in_audience }}"
    tags = "{{ tags }}"
    ttl = "{{ ttl }}"
}