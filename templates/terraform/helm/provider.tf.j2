provider "helm" {
    burst_limit = "{{ burst_limit }}"
    debug = "{{ debug }}"
    helm_driver = "{{ helm_driver }}"
    plugins_path = "{{ plugins_path }}"
    registry_config_path = "{{ registry_config_path }}"
    repository_cache = "{{ repository_cache }}"
    repository_config_path = "{{ repository_config_path }}"
}